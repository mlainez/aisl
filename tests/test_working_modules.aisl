(module test_working_modules
  (import math)
  (import array_utils)
  (import conversion)
  (import string_utils)
  (import http)
  (import json_utils)
  
  (fn main -> int
    (call print "=== Testing math ===")
    (set abs_val int (call abs -42))
    (call print abs_val)
    
    (call print "=== Testing array_utils ===")
    (set arr array (call array_range 1 6))
    (set sum int (call array_sum arr 5))
    (call print sum)
    
    (call print "=== Testing conversion ===")
    (set miles float (call kilometers_to_miles 10.0))
    (call print miles)
    
    (call print "=== Testing string_utils ===")
    (set rev string (call string_reverse "hello"))
    (call print rev)
    
    (call print "=== Testing http ===")
    (set status_text string (call get_status_text 200))
    (call print status_text)
    
    (call print "=== Testing json_utils ===")
    (set obj json (call build_object_one "test" "value"))
    (set obj_str string (call to_string obj))
    (call print obj_str)
    
    (call print "All tests completed!")
    (ret 0)))
