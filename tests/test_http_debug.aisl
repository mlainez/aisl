(module test_http_debug
  (import http)
  
  (fn main -> int
    (call print "Testing HTTP module")
    (call print "Calling get_status_text with 200")
    (set status_text string (call get_status_text 200))
    (call print "Result:")
    (call print status_text)
    
    (call print "Testing direct eq:")
    (set test_eq bool (call eq 200 200))
    (call print test_eq)
    
    (ret 0)))
