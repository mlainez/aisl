(module demo_module_system
  (fn main -> int
    (call print "=== AISL Module System Demo ===")
    
    (set a int 10)
    (set b int 5)
    
    (set sum int (call add a b))
    (call print "10 + 5 = ")
    (call print sum)
    
    (set diff int (call sub a b))
    (call print "10 - 5 = ")
    (call print diff)
    
    (set is_greater bool (call gt a b))
    (call print "10 > 5 = ")
    (call print is_greater)
    
    (call print "All core operations work without imports!")
    
    (ret 0))
  
  (meta-note "Demo: Core operations (add, sub, gt, print) work without imports"))
