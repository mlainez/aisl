(mod test_polymorphic
  (fn main () -> i32
    (set x i32 10)
    (set y i32 20)
    (set sum_i32 i32 (call add x y))
    (call io_print_i32 sum_i32)
    
    (set a f64 3.14)
    (set b f64 2.86)
    (set sum_f64 f64 (call add a b))
    (call io_print_f64 sum_f64)
    
    (set diff i32 (call sub y x))
    (call io_print_i32 diff)
    
    (set prod f64 (call mul a b))
    (call io_print_f64 prod)
    
    (ret 0)))
