(module test_stack
  (fn test_loop count int -> int
    (set i int 0)
    (while (call lt i count)
      (call print ".")
      (set i int (call add i 1)))
    (ret 0))
  
  (fn main -> int
    (call print "Testing loop with 120 iterations...")
    (call test_loop 120)
    (call print "Done!")
    (ret 0)))
