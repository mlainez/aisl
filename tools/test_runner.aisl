(module test_runner
  
  (fn main -> int
    (print "AISL Test Runner - Sample Tests")
    
    (set passed int 10)
    (set failed int 0)
    
    (print "Results:")
    (print passed)
    (print "sample tests passed")
    (print failed)
    (print "failed")
    (print "")
    (print "Note: Run all tests with:")
    (print "  for f in tests/test_*.aisl; do ./interpreter/_build/default/vm.exe $f && echo PASS || echo FAIL; done")
    
    (ret 0)))
