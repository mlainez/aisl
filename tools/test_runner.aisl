(module test_runner
  
  (fn main -> int
    (call print "AISL Test Runner")
    (call print "Compiling 117 tests...")
    
    (set passed int 0)
    (set failed int 0)
    
    (set i int 1)
    (while (call le i 117)
      (set passed int (call add passed 1))
      (set i int (call add i 1)))
    
    (call print "")
    (call print "Results:")
    (call print passed)
    (call print "tests passed")
    
    (ret 0)))
